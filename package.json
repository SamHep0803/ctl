{
  "private": true,
  "scripts": {
    "dev": "yarn dev:local",
    "dev:local": "dotenv -e environments/.env.local next dev",
    "dev:staging": "dotenv -e environments/.env.staging next dev",
    "build": "next build",
    "start": "next start",
    "init-db:local": "dotenv -e environments/.env.local prisma db push && dotenv -e environments/.env.local prisma db seed",
    "init-db:staging": "dotenv -e environments/.env.staging prisma db push && dotenv -e environments/.env.staging prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@chakra-ui/theme-tools": "^1.3.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^3.13.0",
    "apollo-server-micro": "^3.6.7",
    "framer-motion": "^6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "latest",
    "next-auth": "^4.3.4",
    "nexus": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.0",
    "dotenv-cli": "^5.1.0",
    "graphql": "^16.4.0",
    "prettier": "^2.5.1",
    "prisma": "^3.13.0",
    "ts-node": "^10.7.0",
    "typescript": "4.5.4"
  }
}
